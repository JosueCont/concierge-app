{
  "expo": {
    "name": "hybridstoreUpmy",
    "slug": "hybridstoreUpmy",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "checkAutomatically": "ON_ERROR_RECOVERY",
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "versionCode": 1,
      "package": "com.itnovare.upmy",
      "useNextNotificationsApi": true
    },
    "ios": {
      "bundleIdentifier": "com.itnovare.upmy",
      "supportsTablet": true
    }
  }
}
